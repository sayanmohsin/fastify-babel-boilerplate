{
  "name": "fastify-babel-boilerplate",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "NODE_ENV=development nodemon -r ./env/init --exec babel-node ./src/app",
    "prod": "npm run build && NODE_ENV=production nodemon ./dist/app",
    "build": "babel src -d dist --copy-files",
    "start": "npm run prod"
  },
  "author": "SayanMohsin",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.9.2",
    "dotenv": "^8.2.0",
    "fastify": "^2.13.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/generator": "^7.9.4",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-async-generator-functions": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "nodemon": "^2.0.2"
  }
}
